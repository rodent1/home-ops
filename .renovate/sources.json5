{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      matchPackageNames: [ "/actualbudget/" ],
      sourceUrl: "https://github.com/actualbudget/actual",
    },
    {
      matchPackageNames: [ "advplyr/audiobookshelf" ],
      sourceUrl: "https://github.com/advplyr/audiobookshelf",
    },
    {
      matchPackageNames: [ "twin/gatus" ],
      sourceUrl: "https://github.com/TwiN/gatus",
    },
    {
      matchPackageNames: [ "getmeili/meilisearch" ],
      sourceUrl: "https://github.com/meilisearch/meilisearch",
    },
    {
      matchPackageNames: [ "/immich-app/" ],
      sourceUrl: "https://github.com/immich-app/immich",
    },
    {
      matchPackageNames: [ "/jellyfin/" ],
      sourceUrl: "https://github.com/jellyfin/jellyfin",
    },
    {
      matchPackageNames: [ "jellystat" ],
      sourceUrl: "https://github.com/CyferShepard/Jellystat",
    },
  ],
}
