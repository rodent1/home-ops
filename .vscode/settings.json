{
  "files.associations": {
    "**/*.json5": "json5"
  },
  "sops.defaults.ageKeyFile": "age.key",
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "enabled"
  },
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    "kubernetes": "./kubernetes/**/*.yaml"
  },
  "[json5]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "bluebrown.yamlfmt"
  }
}
